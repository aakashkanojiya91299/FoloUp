# Production Environment Variables
# Copy this file to .env and fill in your production values

# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-production-supabase.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# AI Service Configuration
OPENAI_API_KEY=your_production_openai_api_key
GEMINI_API_KEY=your_production_gemini_api_key
AI_PROVIDER=openai

# Retell Configuration
RETELL_API_KEY=your_production_retell_api_key

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_production_clerk_key
CLERK_SECRET_KEY=sk_test_your_production_clerk_secret

# Application Configuration
NODE_ENV=production
PORT=3000
NEXT_PUBLIC_BASE_URL=https://your-domain.com

# Production Security
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_MONITORING=true

# Performance Optimization
NEXT_PUBLIC_ENABLE_CACHE=true
NEXT_PUBLIC_ENABLE_CDN=true

# Monitoring and Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
SENTRY_DSN=https://your_sentry_dsn_here
NEXT_PUBLIC_SENTRY_DSN=https://your_sentry_dsn_here

# Rate Limiting
NEXT_PUBLIC_RATE_LIMIT_ENABLED=true
NEXT_PUBLIC_MAX_REQUESTS_PER_MINUTE=100

# Security Headers
NEXT_PUBLIC_ENABLE_CSP=true
NEXT_PUBLIC_ENABLE_HSTS=true 